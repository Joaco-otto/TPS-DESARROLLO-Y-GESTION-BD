							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"marca\": \"Honda\",\n    \"precio\": 28000.00\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/autos/{{auto_id}}",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"autos",
								"{{auto_id}}"
							]
						},
						"description": "Actualiza un auto existente por su ID"
					},
					"response": []
				},
				{
					"name": "DELETE - Eliminar auto",
					"request": {
						"method": "DELETE",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/autos/{{auto_id}}",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"autos",
								"{{auto_id}}"
							]
						},
						"description": "Elimina un auto de la base de datos por su ID"
					},
					"response": []
				}
			]
		},
		{
			"name": "Consultas Específicas",
			"item": [
				{
					"name": "GET - Buscar autos por rango de precio",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/autos/precio?min={{precio_min}}&max={{precio_max}}",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"autos",
								"precio"
							],
							"query": [
								{
									"key": "min",
									"value": "{{precio_min}}",
									"description": "Precio mínimo del rango de búsqueda"
								},
								{
									"key": "max",
									"value": "{{precio_max}}",
									"description": "Precio máximo del rango de búsqueda"
								}
							]
						},
						"description": "Busca autos dentro de un rango específico de precios"
					},
					"response": []
				}
			]
		},
