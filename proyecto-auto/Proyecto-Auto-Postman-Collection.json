				{
					"name": "GET - Obtener todos los autos",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/autos",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"autos"
							]
						},
						"description": "Obtiene la lista completa de todos los autos registrados en la base de datos"
					},
					"response": []
				},
				{
					"name": "GET - Obtener auto por ID",
					"request": {
						"method": "GET",
