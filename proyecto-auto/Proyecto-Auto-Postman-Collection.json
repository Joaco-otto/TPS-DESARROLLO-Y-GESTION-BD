						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"marca\": \"Toyota\",\n    \"precio\": 25000.00\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/autos",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"autos"
							]
						},
						"description": "Crea un nuevo auto en la base de datos"
					},
					"response": []
				},
				{
					"name": "PUT - Actualizar auto existente",
					"request": {
						"method": "PUT",
